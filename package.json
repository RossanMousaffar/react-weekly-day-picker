  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "start": "cross-env BABEL_ENV=es NODE_ENV=development rollup -w -c example/rollup.config.js",
    "docs:build": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -w example/rollup.config.js",
    "docs:publish": "npm run docs:build && cd example && rimraf .git && git init && git commit --allow-empty -m 'update docs' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update docs' && git push git@github.com:andreypopp/react-weekly-day-picker gh-pages --force",
    "size": "size-limit",
    "update:size": "cross-env ROLLUP_BUILDS=browser_esm npm run build && node scripts/update-size.js && git add README.md",
    "lint": "eslint src",
    "lint:watch": "esw -w --fix src",
    "preversion": "npm run lint",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy:docs": "gh-pages -d styleguide",
    "update-coc": "weallbehave -o ."
  },
